{
  "name": "thru-space",
  "description": "Travel through space at light speed!",
  "keywords": [
    "animation",
    "canvas",
    "fly",
    "universe",
    "space",
    "stars",
    "light",
    "speed"
  ],
  "version": "0.0.4",
  "type": "module",
  "author": "Ivan Che",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gVguy/thru-space.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "vite build -c vite-demo.config.ts",
    "preview": "vite preview"
  },
  "main": "./dist/thru-space.umd.cjs",
  "module": "./dist/thru-space.js",
  "types": "./dist/thru-space.d.ts",
  "exports": {
    ".": {
      "import": "./dist/thru-space.js",
      "types": "./dist/thru-space.d.ts",
      "require": "./dist/thru-space.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  }
}
